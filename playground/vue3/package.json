    "zod": "3.22.4"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.7.0",
    "@tsconfig/node18": "^18.2.2",
    "@types/node": "^20.11.5",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.20.1",
    "npm-run-all2": "^6.1.1",
    "prettier": "^3.2.4",
    "typescript": "~5.3.3",
    "vite": "^5.0.11",
    "vue-tsc": "1.8.27"
