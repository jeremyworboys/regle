    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^9.0.0",
    "eslint-plugin-vue": "^9.24.1",
    "npm-run-all2": "^6.1.2",
    "prettier": "^3.2.5",
