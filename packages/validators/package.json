{
  "name": "@regle/validators",
  "version": "0.0.12",
  "description": "Vue form validator",
  "scripts": {
    "lint": "eslint --ext .ts --ext .vue .",
    "typecheck": "tsc --noEmit && vitest run --typecheck",
    "release": "pnpm publish --report-summary",
    "build": "tsup --minify",
    "build:local": "tsup --clean false",
    "dev": "tsup --config=tsup.dev.ts --watch",
    "test": "vitest run"
  },
  "dependencies": {
    "@regle/core": "workspace:*"
  },
  "devDependencies": {
    "@types/prettier": "3.0.0",
    "@typescript-eslint/eslint-plugin": "7.6.0",
    "@typescript-eslint/parser": "7.6.0",
    "@vue/test-utils": "2.4.5",
    "bumpp": "9.4.0",
    "changelogithub": "0.13.5",
    "cross-env": "7.0.3",
    "eslint": "9.0.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-vue": "9.24.1",
    "prettier": "3.2.5",
    "tsup": "8.0.2",
    "type-fest": "4.15.0",
    "typescript": "5.4.4",
    "vitest": "1.4.0",
    "vue": "3.4.21",
    "vue-eslint-parser": "9.4.2",
    "vue-tsc": "2.0.11"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": {
    "name": "Victor Garcia",
    "url": "https://github.com/victorgarciaesgi"
  },
  "license": "MIT"
}
