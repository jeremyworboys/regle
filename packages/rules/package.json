{
  "name": "@regle/rules",
  "version": "0.2.13",
  "description": "Collection of rules and helpers for Regle",
  "scripts": {
    "lint": "eslint --ext .ts --ext .vue .",
    "typecheck": "tsc --noEmit",
    "release": "pnpm publish --report-summary",
    "build": "tsup --minify",
    "build:sourcemaps": "tsup --clean false --sourcemap inline",
    "dev": "tsup --config=tsup.dev.ts --watch",
    "test": "vitest run"
  },
  "dependencies": {
    "@regle/core": "workspace:*"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "8.15.0",
    "@typescript-eslint/parser": "8.15.0",
    "@vue/test-utils": "2.4.6",
    "bumpp": "9.8.1",
    "changelogithub": "0.13.11",
    "cross-env": "7.0.3",
    "type-fest": "4.28.0",
    "vitest": "2.1.5",
    "eslint": "catalog:",
    "eslint-config-prettier": "catalog:",
    "eslint-plugin-vue": "catalog:",
    "prettier": "catalog:",
    "tsup": "catalog:",
    "typescript": "catalog:",
    "vue": "catalog:",
    "@vue/reactivity": "catalog:",
    "vue-eslint-parser": "catalog:",
    "vue-tsc": "catalog:"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": {
    "name": "Victor Garcia",
    "url": "https://github.com/victorgarciaesgi"
  },
  "homepage": "https://regle.vercel.app/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/victorgarciaesgi/regle.git"
  },
  "license": "MIT"
}