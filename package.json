{
  "name": "regle",
  "version": "0.0.9",
  "private": true,
  "description": "Vue form validator",
  "scripts": {
    "lint": "eslint --ext .ts --ext .vue .",
    "typecheck": "pnpm run -r --parallel typecheck",
    "build": "pnpm run -r build",
    "build:local": "pnpm run -r build:local",
    "dev": "pnpm run -r --parallel dev",
    "test": "pnpm run build && vitest run",
    "test:dev": "vitest",
    "release": "nr build && bumpp -r && pnpm run -r --parallel release && git push --follow-tags"
  },
  "devDependencies": {
    "@regle/core": "workspace:*",
    "@regle/validators": "workspace:*",
    "@types/prettier": "3.0.0",
    "@typescript-eslint/eslint-plugin": "6.19.0",
    "@typescript-eslint/parser": "6.19.0",
    "@vue/test-utils": "2.4.3",
    "bumpp": "9.3.0",
    "changelogithub": "0.13.3",
    "cross-env": "7.0.3",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-vue": "9.20.1",
    "prettier": "3.2.4",
    "typescript": "5.3.3",
    "vitest": "1.2.1",
    "vue": "3.4.14",
    "vue-eslint-parser": "9.4.0",
    "vue-tsc": "1.8.27"
  },
  "type": "module",
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": {
    "name": "Victor Garcia",
    "url": "https://github.com/victorgarciaesgi"
  },
  "license": "MIT"
}
